import java .util.*;
class Solution {
public static void main(String args[])
{
String str;
Scanner sc=new Scanner(System.in);
System.out.print("Enter string data:");
str=sc.nextLine();
int sum=0;

String ch[]=str.split(" ");
int n=ch.length;
if(n==0)
System.out.print(n);
else
{
String st=ch[n-1];
sum=+st.length();
int m=st.length();
System.out.print(str.replace(" ","").length());

}
}
}
